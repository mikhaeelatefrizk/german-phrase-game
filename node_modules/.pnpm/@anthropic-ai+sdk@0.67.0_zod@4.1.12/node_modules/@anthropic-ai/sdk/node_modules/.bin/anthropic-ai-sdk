#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/sdk/bin/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/sdk/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/sdk/bin/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/sdk/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules/@anthropic-ai/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/@anthropic-ai+sdk@0.67.0_zod@4.1.12/node_modules:/home/ubuntu/german-phrase-game-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
